 <!DOCTYPE html>
  <html>
  <head>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <style>
  * {
    box-sizing: border-box;
  }

  /* Create two equal columns that floats next to each other */
  .column {
    border-radius: 15px;
    float: left;
    width: 49.75%;
    height: 300px; /* Should be removed. Only for demonstration */
  }

  .columnGap {
    float: left;
    width: 0.5%;
    height: 300px; /* Should be removed. Only for demonstration */
  }

  /* Clear floats after the columns */
  .row:after {
    float: left
    content: "";
    display: table;
    clear: both;
  }
  </style>
  </head>
  <body>

  <div class="row">
    <div class="column" style="background-color:#aaa;">
      <div class="movement" style= "margin-top:10px">
        <form method="POST" action="/api/movement">
          <label for="x">x position:</label>
          <input type="text" id="x" name="x" value="999">
          <label for="y">y position:</label>
          <input type="text" id="y" name="y" value="9999">
          <input type="submit" value="submit">
       </form>
     </div>
    </div>
    <div class="columnGap" style="background-color:#fff	;">
    </div>
    <div class="column" style="background-color:#bbb;">
      <h2>Column 2</h2>
      <p>Some text..</p>
    </div>
  </div>

  </body>
  </html>
